import java.util.*;
 class Day10 {
   //main method
   public static void main(String args[])
   { 
    System.out.println("Day10 byte");
    //variabel deklarasi
    byte b1,b2,b3;
    //variabel inisialisasi
    b1 = 10;
    b2 = 15;
    b3 = 27;
    //printf
    System.out.printf("\n%d \n%d \n%d\n",b1,b2,b3);
    //println
    System.out.println("Total:"+b1+b2+b3);
    //variabel update 
    b1 = 9;
    b2 = 13;
    b3 = 25;
    System.out.println("nilai Setelah update");
    System.out.printf("%d \n%d \n%d\n",b1,b2,b3);
    System.out.println("Total"+b1+b2+b3);
   }
 }
